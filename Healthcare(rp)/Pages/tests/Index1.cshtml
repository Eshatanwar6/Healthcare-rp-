@page
@model Healthcare_rp_.tests.IndexModel
@{
}

<br>
<h2> List  of Test </h2>
<a class='btn btn-primary btn-sm' href='/tests/Create1'>New Test</a>
<table class="table">
	<thead>
		<tr>
			<th>Test Id</th>

			<th>Test Name</th>
			
		</tr>

	</thead>
	<tbody>
		@foreach (var item in Model.TestList)
		{
			<tr>
				<td>@item.TestId</td>
				<td>@item.TestName</td>
				<td>
					<a class="btn btn-primary btn-sm" href="/tests/Edit?TestId=@item.TestId">Edit</a>
					<a class="btn btn-primary btn-sm" href="/tests/Delete?TestId=@item.TestId">Delete</a>
				</td>
			</tr>

		}
	</tbody>

</table>